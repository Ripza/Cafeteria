<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>CalendarioControllerBean</managed-bean-name>
        <managed-bean-class>controladores.CalendarioController</managed-bean-class>
    
        <managed-bean-scope></managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>OrderControllerBean</managed-bean-name>
        <managed-bean-class>controladores.OrderController</managed-bean-class>
    
        <managed-bean-scope></managed-bean-scope></managed-bean>
    
    
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle2</base-name>
            <var>bundle2</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>ComidaControllerBean</managed-bean-name>
        <managed-bean-class>controladores.ComidaController</managed-bean-class>
    </managed-bean>
</faces-config>
